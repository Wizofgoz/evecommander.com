{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "cloudcreativity/laravel-json-api": "^1.0",
        "evecommander/esi-client-php": "^0.8.1",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "irazasyed/jwt-auth-guard": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^2.0",
        "laravel/passport": "^7.2",
        "laravel/tinker": "~1.0",
        "moontoast/math": "^1.1",
        "owen-it/laravel-auditing": "^8.0",
        "predis/predis": "^1.1",
        "shipping-docker/vessel": "^3.1",
        "tymon/jwt-auth": "^1.0.0-rc.3",
        "web-token/jwt-checker": "^1.3",
        "web-token/jwt-core": "^1.3",
        "web-token/jwt-signature": "^1.3"
    },
    "require-dev": {
        "cloudcreativity/json-api-testing": "1.0.0-rc.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
